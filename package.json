{
	"name": "virtual-labs-experiment-authoring-environment",
	"displayName": "Virtual Labs Experiment Authoring Environment",
	"description": "A VS Code extension to handle all the common tasks invovled in virtual labs experiment development.",
	"version": "0.0.1",
	"publisher": "Virtual-Labs",
	"repository": "https://github.com/Dileepadari/virtual_web_trial.git",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:virtual-labs-experiment-authoring-environment.virtualLabs"
	],
	"browser": "./dist/web/extension.js",
	"icon": "images/logo-for-vlabs.png",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "vlabs-experiment-generator",
					"title": "Experiment",
					"icon": "images/icon.svg"
				}
			]
		},
		"views": {
			"vlabs-experiment-generator": [
				{
					"type": "webview",
					"id": "vlabs.experimentView",
					"name": "Authoring Environment"
				}
			]
		},
		"commands": [
			{
				"command": "virtual-labs-experiment-authoring-environment.virtualLabs",
				"title": "Virtual Labs",
				"icon": {
					"light": "images/icon.svg",
					"dark": "images/icon.svg"
				}
			}
		]
	},
	"scripts": {
		"dev": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=.",
		"test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
		"pretest": "npm run compile-web",
		"vscode:prepublish": "npm run package-web",
		"compile-web": "webpack",
		"watch-web": "webpack --watch",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"lint": "eslint src --ext ts"
	},
	"devDependencies": {
		"@types/mocha": "^9.0.0",
		"@types/vscode": "^1.73.0",
		"@types/webpack-env": "^1.16.2",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"@vscode/test-web": "^0.0.22",
		"assert": "^2.0.0",
		"eslint": "^8.26.0",
		"mocha": "^9.2.0",
		"process": "^0.11.10",
		"ts-loader": "^9.2.5",
		"typescript": "^5.3.2",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"axios": "^1.6.7",
		"fs": "^0.0.1-security",
		"jszip": "^3.10.1",
		"shelljs": "^0.8.5",
		"simple-git": "^3.22.0",
		"unzipper": "^0.10.14"
	}
}
